{% extends '::layout.html.twig' %}
{% block feedback %}
    <div class="container">
        <div class="row">
            <div class="col-md-6">
                {{ form_start(form, {'action': '/'}) }}
                    <div class="form-group">
                        {{ form_row(form.name) }}
                    </div>
                    <div class="form-group">
                        {{ form_row(form.age) }}
                    </div>
                    <div class="form-group">
                        {{ form_row(form.email) }}
                    </div>
                    <div class="form-group">
                        {{ form_row(form.date) }}
                    </div>
                    {#<div class="form-group">#}
                        {#{{ form_row(form.file) }}#}
                    {#</div>#}

                    {#{{ form_row(form.save) }}#}

                    <button type="submit" class="btn btn-success">Отправить</button>
                {{ form_end(form) }}
            </div>
        </div>
    </div>
{% endblock %}
